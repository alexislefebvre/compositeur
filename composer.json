{
  "name": "alexislefebvre/compositeur",
  "type": "composer-plugin",
  "require": {
    "php": "^7.0",
    "composer-plugin-api": "^1.1"
  },
  "require-dev": {
    "composer/composer": "^1.0",
    "phpunit/phpunit": "^7.2"
  },
  "extra": {
    "class": "Compositeur\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "Compositeur\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Compositeur\\Tests\\": "tests"
    }
  }
}